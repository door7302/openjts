      "cos_forwarded_octets_rate",
      "cos_red_dropped_packets_rate",
      "cos_tail_dropped_packets_rate",
      "cos_forwarded_packets_rate",
      "cos_buffer_size",
      "cos_buffer_peak",
      "cos_buffer_cur",
      "cos_buffer_avg",
      "mcast_in_pkts_rate",
      "mcast_out_pkts_rate",
      "bcast_in_pkts_rate",
