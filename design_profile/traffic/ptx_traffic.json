  "filtering_list": [{
    "order": 200,
    "namepass": ["INTERFACES", "SUBINTERFACES"],
    "filters": [{
      "type": 0,
      "key": "if_name",
      "pattern": "^(et-|xe-|ge-|ae|lt-|ps-|fti-|gr-|demux).*",
      "action": "accept"
    }]
  }],
  "enum_list": [{
    "order": 300,
    "namepass": ["INTERFACES"],
    "entries": [{
      "tag": "queue_id",
      "dest": "queue_name"
    }]
  }],
  "enrichment_list": [{
    "order": 400,
    "namepass": ["INTERFACES", "SUBINTERFACES"],
    "family": "ptx",
    "two_levels": true,
    "level1_tag": "device",
    "level2_tags": ["if_name"]
  }],
  "rate_list": [{
    "order": 500,
    "namepass": ["INTERFACES", "SUBINTERFACES"],
    "fields": ["in_pkts", "out_pkts", "in_octets", "out_octets", "unit_in_pkts", "unit_out_pkts", "unit_in_octets", "unit_out_octets", "mcast_in_pkts", "mcast_out_pkts", "bcast_in_pkts", "bcast_out_pkts", "cos_forwarded_octets", "cos_forwarded_packets", "cos_queued_octets", "cos_queued_packets", "cos_red_dropped_packets", "cos_tail_dropped_packets", "in_errors", "out_errors"]

  }],
  "monitoring_list": [{
    "order": 600,
    "namepass": ["INTERFACES", "SUBINTERFACES"],
    "probes": [{
        "name": "RED_DROP",
        "field": "cos_red_dropped_packets_rate",
        "type": "current",
        "threshold": 1,
        "operator": "gt",
        "tags": ["device", "SHORTNAME", "if_name", "queue_id", "queue_name", "LINKNAME"]
      },
      {
        "name": "TAIL_DROP",
        "field": "cos_tail_dropped_packets_rate",
        "type": "current",
        "threshold": 1,
        "operator": "gt",
        "tags": ["device", "SHORTNAME", "if_name", "queue_id", "queue_name", "LINKNAME"]
      },
      {
        "name": "ETH_DROP_IN",
        "field": "in_errors_rate",
        "type": "current",
        "threshold": 1,
        "operator": "gt",
        "tags": ["device", "SHORTNAME", "if_name", "LINKNAME"]
      },
      {
        "name": "ETH_DROP_OUT",
        "field": "out_errors_rate",
        "type": "current",
        "threshold": 1,
        "operator": "gt",
        "tags": ["device", "SHORTNAME", "if_name", "LINKNAME"]
      }
    ]
  }],
  "influx_outputs": [{
    "retention": "autogen",
    "fieldpass": [
      "exception",
      "in_octets_rate",
      "out_octets_rate",
      "in_pkts_rate",
      "out_pkts_rate",
      "unit_in_octets_rate",
      "unit_out_octets_rate",
      "unit_in_pkts_rate",
      "unit_out_pkts_rate",
      "in_errors_rate",
      "out_errors_rate",
      "cos_queued_packets_rate",
