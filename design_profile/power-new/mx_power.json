{
    "gnmi_inputs": [
      {
        "subscriptions": [
          {
            "name": "COMPONENTS",
            "path": "/components/component",
            "mode": "sample",
            "interval": 60
          },
          {
            "name": "INTERFACES",
            "path": "/interfaces/interface/state",
            "mode": "sample",
            "interval": 60
          }
        ]
      }
    ],
    "pivot_list": [
    {
      "order": 10,
      "namepass": ["COMPONENTS"],
      "tag": "/components/component/properties/property/name",
      "field": "/components/component/properties/property/state/value"
    }
    ],
